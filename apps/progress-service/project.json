{
  "name": "progress-service",
  "root": "apps/progress-service",
  "projectType": "application",
  "sourceRoot": "apps/progress-service/src",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/progress-service",
        "main": "apps/progress-service/src/main.ts",
        "tsConfig": "apps/progress-service/tsconfig.app.json",
        "assets": ["apps/progress-service/src/assets"]
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "progress-service:build"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{options.outputPath}"],
      "options": {
        "jestConfig": "apps/progress-service/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
